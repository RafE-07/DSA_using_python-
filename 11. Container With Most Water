class Solution:
    def maxArea(self, height: List[int]) -> int:
        n = len(height)
        i=0
        j=n-1
        maxwater = 0
        while (i<j):
            w = j-i
            h = min(height[i], height[j])
            area = w * h
            maxwater = max(maxwater,area)
            if(height[i] >height[j]):
                j=j-1
            else:
                i=i+1   
        return maxwater
        
